<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="DxDataFormat.Frm" targetNamespace="http://tempuri.org/DxDataFormat.Frm.xsd" elementFormDefault="qualified" xmlns="http://tempuri.org/DxDataFormat.Frm.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:annotation>
        <xs:documentation xml:lang="en">
            Part of Helios Nephrite, proprietary software, (c) Asseco Solutions, a. s.
            DxDataFormat template schema.
            All rights reserved.
        </xs:documentation>
    </xs:annotation>

	<!--    ROOT ELEMENT šablony                                                                             -->
	<xs:element name="template" type="type_form" >
		<xs:annotation>
			<xs:documentation xml:lang="cs-cz">Definice celé šablony (formuláře). Může obsahovat jednotlivé stránky (page) anebo panely (panel). Pokud v šabloně nebude definována žádná 'page', pak bude zobrazen formulář bez záložek.</xs:documentation>
		</xs:annotation>
	</xs:element>

    
    <!--    DEFINICE KONTEJNERŮ                                                                              -->

	<!--    type_form = DfForm   -  celá šablona                                                             -->
	<xs:complexType name="type_form">
		<xs:complexContent>
			<xs:extension base="type_base_area">
				<xs:sequence>
					<!--    V šabloně se mohou střídat libovolně elementy page a panel a nestedpanel         -->
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="page" type="type_page" minOccurs="0" maxOccurs="unbounded" >
							<xs:annotation>
								<xs:documentation xml:lang="cs-cz">Jeden objekt s více záložkami (page). Pokud v šabloně nebude definována žádná 'page', pak bude zobrazen formulář bez záložek. Stránka 'page' smí obsahovat pouze panely.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="panel" type="type_panel" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="cs-cz">Jeden vizuální panel. Obsahuje jednotlivé prvky anebo vnořené grupy. Panel smí být umístěn buď v šabloně, anebo v konkrétní stránce 'page'.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="nestedpanel" type="type_nested_panel" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="cs-cz">Vnořený panel, jehož obsah definuje jiná šablona (NestedTemplate).</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>

				<xs:attribute name="Name" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Identifikátor šablony není povinný.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="FormatVersion" type="format_version_enum" default="4">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Formát tohoto souboru. Defaultní = 4.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="MasterWidth" type="xs:int">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Šířka vlastní oblasti DataFormu = Master (tedy bez dynamických vztahů).</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="MasterHeight" type="xs:int">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Výška vlastní oblasti DataFormu = Master (tedy bez dolních záložek).</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="TotalWidth" type="xs:int">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Celková šířka okna (tedy včetně dynamických vztahů).</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="TotalHeight" type="xs:int">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Celková výška okna (tedy včetně dolních záložek).</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AutoLabelPosition" type="label_position_enum" >
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Automaticky generovat labely atributů a vztahů, jejich umístění. Defaultní = None.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="DataSource" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Jméno souboru, z něhož bude načtena definice dat.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Messages" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Jméno souboru, z něhož budou čteny překlady popisků a tooltipů.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="UseNorisClass" type="xs:int">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Číslo třídy, pro kterou budou načteny popisky atributů a vztahů.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AddUda" type="bool_enum">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Automaticky přidat záložku pro UDA atributy a vztahy. Nezadáno = True.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="UdaLabelPosition" type="label_position_enum">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Umístění labelů pro automaticky generované UDA atributy a vztahy. Defaultní = Top.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Margins" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Vnitřní okraje uvnitř containeru: mezi vnější souřadnicí a prvním prvkem na souřadnici 0/0. Ve formě: All   nebo   Left,Top[,Right,Bottom].</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ContextMenu" type="bool_enum">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Povolit kontextové menu.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--    type_page = DfPage   -  jedna stránka                                                            -->
    <xs:complexType name="type_page">
        <xs:complexContent>
            <xs:extension base="type_base_area">
                <xs:sequence>
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="panel" type="type_panel" minOccurs="0" maxOccurs="unbounded" >
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Jeden panel uvnitř stránky, uživatelsky viditelný včetně titulku.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="nestedpanel" type="type_nested_panel" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Vnořený panel, jehož obsah definuje jiná šablona (NestedTemplate).</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:sequence>

                <xs:attribute name="IconName" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název ikony. Zobrazuje se v záhlaví před textem.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Title" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Titulek záhlaví stránky. Může obsahovat formátovací funkce fm(), fmr(), fmdr().</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
			</xs:extension>
        </xs:complexContent>
    </xs:complexType>

	<!--    type_panel = DfPanel  -  Panel                                                                   -->
    <xs:complexType name="type_panel">
        <xs:complexContent>
            <xs:extension base="type_base_container">
                <xs:attribute name="IsHeader" type="bool_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Obsahuje true, pokud tento panel reprezentuje záhlaví. To může být vykresleno na každé záložce, vždy jako první horní panel, v šířce přes všechny sloupce.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="HeaderOnPages" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název stránek (Page.Name), na kterých je toto záhlaví zobrazeno. Má význam pouze pokud IsHeader je true. Pokud nebude HeaderOnPages zadáno, bude vykresleno pro všechny stránky.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="IconName" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název ikony. Zobrazují ji pouze některé prvky.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Title" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Titulek panelu. Může obsahovat formátovací funkce fm(), fmr(), fmdr().</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="TitleStyle" type="title_style_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Styl titulku.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="TitleColorName" type="color_cup_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Barva textu titulku, zadaná jako název kalíšku.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="TitleColorLight" type="color_name_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Barva textu titulku, zadaná explicitně pro světlé skiny: buď jménem např. Red, LightGray, anebo jako RGB: 0xDDFFDD, atd.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="TitleColorDark" type="color_name_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Barva textu titulku, zadaná explicitně pro tmavé skiny: buď jménem např. Red, LightGray, anebo jako RGB: 0xDDFFDD, atd.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="CollapseState" type="panel_collapse_state_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Možnosti přeskupování panelu (Collapse / Expand).</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

	<!--    type_nested_panel = DfNestedPanel = Vnořený panel, vložený do Page.   -->
	<xs:complexType name="type_nested_panel">
		<xs:complexContent>
			<xs:extension base="type_base">
				<xs:attribute name="NestedTemplate" type="xs:string" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Název souboru, který obsahuje definici vnořené šablony.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="NestedPanelName" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Jméno hledaného panelu v nested šabloně. Pokud není zadáno, vezme se první nalezený panel.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="IsHeader" type="bool_enum">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Obsahuje true, pokud tento panel reprezentuje záhlaví. To může být vykresleno na každé záložce, vždy jako první horní panel, v šířce přes všechny sloupce.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="HeaderOnPages" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Název stránek (Page.Name), na kterých je toto záhlaví zobrazeno. Má význam pouze pokud IsHeader je true. Pokud nebude HeaderOnPages zadáno, bude vykresleno pro všechny stránky.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--    type_group = DfGroup  -  Grupa uvnitř Panelu nebo uvnitř grupy, ale bez chování Panelu = nemá Collapsed a Title    -->
    <xs:complexType name="type_group">
        <xs:complexContent>
            <xs:extension base="type_base_container">
                <xs:attribute name="ColIndex" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Index sloupce, na kterém je prvek umístěn v režimu FlowLayout. Ten se použije, pokud prvky nemají exaktně dané souřadnice, spolu s atributem 'ColumnWidths'. První sloupec má index 0. Zadáním indexu sloupce lze povinně přeskočit některé columny, anebo přeskočit na další řádek.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ColSpan" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Počet sloupců, které prvek obsazuje v režimu FlowLayout. Ten se použije, pokud prvky nemají exaktně dané souřadnice, spolu s atributem 'ColumnWidths'.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="RowSpan" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Počet řádků, které prvek obsazuje v režimu FlowLayout. Ten se použije, pokud prvky nemají exaktně dané souřadnice.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="HPosition" type="position_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Umístění prvku vodorovně v rámci sloupce v případě, kdy šířka prvku je menší než šířka sloupce. Řeší tedy zarovnání controlu ve sloupci, nikoli zarovnání obsahu v rámci controlu.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_nested_group = DfNestedGroup = z hlediska DataFormu jde o Container, ale z hlediska zadávání o jednotlivý Control = nemá vnitřní prvky (elementy)   -->
    <xs:complexType name="type_nested_group">
        <xs:complexContent>
            <xs:extension base="type_base">
                <xs:attribute name="NestedTemplate" type="xs:string" use="required">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název souboru, který obsahuje definici vnořené šablony.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
				<xs:attribute name="NestedGroupName" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Jméno hledané grupy v nested šabloně. Pokud není zadáno, vezme se první nalezená grupa.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Bounds" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice prvku v jednom políčku, v pořadí: X,Y,Width[,Height]. Alternativa hodnot X, Y, Width, Height.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Size" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Velikost prvku v jednom políčku, v pořadí: Width,Height. Alternativa hodnot Bounds, Width, Height.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="X" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Y" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Width" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Šířka prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds. Pouze do atributu Width lze zadat šířku v % z celkové šířky vyhrazeného sloupce. To platí pouze pro umístění do FlowLayout, nikoli FixedLayout.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Height" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Výška prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ColIndex" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Index sloupce, na kterém je prvek umístěn v režimu FlowLayout. Ten se použije, pokud prvky nemají exaktně dané souřadnice, spolu s atributem 'ColumnWidths'. První sloupec má index 0. Zadáním indexu sloupce lze povinně přeskočit některé columny, anebo přeskočit na další řádek.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ColSpan" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Počet sloupců, které prvek obsazuje v režimu FlowLayout. Ten se použije, pokud prvky nemají exaktně dané souřadnice, spolu s atributem 'ColumnWidths'.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="RowSpan" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Počet řádků, které prvek obsazuje v režimu FlowLayout. Ten se použije, pokud prvky nemají exaktně dané souřadnice.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="HPosition" type="position_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Umístění prvku vodorovně v rámci sloupce v případě, kdy šířka prvku je menší než šířka sloupce. Řeší tedy zarovnání controlu ve sloupci, nikoli zarovnání obsahu v rámci controlu.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_base_container = DfBaseContainer  -  Prostor obsahující controly = základ pro Panel a pro Group     -->
    <xs:complexType name="type_base_container">
        <xs:complexContent>
            <xs:extension base="type_base_area">
                <xs:sequence>
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="label" type="type_label" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Neinteraktivní textový popisek.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="title" type="type_title" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Neinteraktivní titulkový prvek.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="checkbox" type="type_checkbox" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Checkbox, zaškrtávátko, různé styly.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="button" type="type_button" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Samostatné tlačítko.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="dropdownbutton" type="type_drop_down_button" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Samostatné tlačítko s přidaným DropDown menu pod-tlačítek.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="textbox" type="type_textbox" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Vstupní textové políčko.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
						<xs:element name="editbox" type="type_editbox" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="cs-cz">Vstupní editorové políčko (editace většího obsahu: poznámka atd).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="textboxbutton" type="type_textboxbutton" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Vstupní textové políčko s buttony. Buttony se zadávají jako vnořený element jména 'rightButton' nebo 'leftButton'.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="combobox" type="type_combobox" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Rozbalovací nabídka typu ComboBox.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
						<xs:element name="image" type="type_image" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="cs-cz">Obrázek. Obsah obrázku může být zadán jako text v atributu 'ImageName', nebo jako přímá binární data v atributu 'ImageData' nebo bude jeho obsah dodán v datovém sloupci.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="stepprogress" type="type_stepprogress" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="cs-cz">StepProgressBar: postupné kroky procesu.</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="group" type="type_group" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Vnořená skupina controlů.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="nestedgroup" type="type_nested_group" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Vnořená grupa, jejíž obsah definuje jiná šablona (NestedTemplate).</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:sequence>

				<xs:attribute name="Bounds" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice prvku v jednom políčku, v pořadí: X,Y,Width[,Height]. Alternativa hodnot X, Y, Width, Height.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Size" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Velikost prvku v jednom políčku, v pořadí: Width,Height. Alternativa hodnot Bounds, Width, Height.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="X" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Y" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Width" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Šířka prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds. Pouze do atributu Width lze zadat šířku v % z celkové šířky vyhrazeného sloupce. To platí pouze pro umístění do FlowLayout, nikoli FixedLayout.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Height" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Výška prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
				<xs:attribute name="FlowLayoutOrigin" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Souřadnice bodu, kde začíná rozmísťování prvků, které nemají zadanou exaktní souřadnici X,Y. Výchozí je "0,0". Zadává se ve formě: "X,Y".</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <!--    type_base_area = DfBaseArea  -  Prostor s něčím uvnitř = základ pro Page, pro Panel a pro Group  -->
    <xs:complexType name="type_base_area">
        <xs:complexContent>
            <xs:extension base="type_base">
                <xs:attribute name="BackColorName" type="color_cup_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Barva pozadí, zadaná jako název kalíšku.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="BackColorLight" type="color_name_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Barva pozadí, zadaná explicitně pro světlé skiny: buď jménem např. Red, LightGray, anebo jako RGB: 0xDDFFDD, atd.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="BackColorDark" type="color_name_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Barva pozadí, zadaná explicitně pro tmavé skiny: buď jménem např. Red, LightGray, anebo jako RGB: 0xDDFFDD, atd.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="BackImageName" type="xs:string" >
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Obrázek vykreslený na pozadí prostoru. Zadán má být jako jméno souboru.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="BackImagePosition" type="back_image_position_enum" >
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Pozice obrázku 'BackImageName' vzhledem k prostoru.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Margins" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Vnitřní okraje uvnitř containeru: mezi vnější souřadnicí a prvním prvkem na souřadnici 0/0. Ve formě: All   nebo   Left,Top[,Right,Bottom].</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ColumnsCount" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">
                            Počet sloupců layoutu. Šířka sloupců se určí podle reálného obsahu (maximum šířky prvků). 
                            Při zadání 'ColumnsCount' se již nezadává 'ColumnWidths'.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ColumnWidths" type="xs:string">
                    <xs:annotation>
						<xs:documentation xml:lang="cs-cz">
                            Šířky jednotlivých sloupců layoutu, oddělené čárkou; např. 150;350;100 (deklaruje tři sloupce dané šířky).
                            Sloupce mohou deklarovat i šířku pro label umístěný před controlem, pak se použije čárka: "100,150; 120,300; 80,100" (deklaruje tři sloupce s danou šířkou labelu a šířkou controlu).
                            Mezery jsou ignorovány.
                            Při zadání 'ColumnWidths' se již nezadává 'ColumnsCount'.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ColumnsDistance" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">
                            Volný prostor v pixelech mezi dvěma sousedními sloupci (vnitřní Margin). Nezadáno = sloupce navazují těsně vedle sebe.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="AutoLabelPosition" type="label_position_enum" >
                    <xs:annotation><xs:documentation xml:lang="cs-cz">Automaticky generovat labely atributů a vztahů v rámci tohoto containeru, jejich umístění. Defaultní = podle parenta.</xs:documentation></xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    
    <!--    DEFINICE JEDNOTLIVÝCH FINÁLNÍCH CONTROLŮ                                                         -->

    <!--    type_title = DfLabel  -  Label                                                                   -->
    <xs:complexType name="type_label">
        <xs:complexContent>
            <xs:extension base="type_base_control">
                <xs:attribute name="Text" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Pevný text prvku (text Labelu). Může obsahovat formátovací funkce fm(), fmr(), fmdr().</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Alignment" type="content_alignment_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Zarovnání obsahu (textu) v definovaném prostoru prvku.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_title = DfTitle  -  Titulkový řádek uvnitř Panelu                                           -->
    <xs:complexType name="type_title">
        <xs:complexContent>
            <xs:extension base="type_base_control">
                <xs:attribute name="Text" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Pevný text prvku = text titulku. Může obsahovat formátovací funkce fm(), fmr(), fmdr().</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="IconName" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název ikony. Zobrazují ji pouze některé prvky.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Style" type="title_style_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Styl titulku.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Alignment" type="content_alignment_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Zarovnání obsahu (titulku) v definovaném prostoru prvku.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <!--    type_checkbox = DfCheckBox  -  CheckBox, RadioButton, Switch                                     -->
    <xs:complexType name="type_checkbox">
        <xs:complexContent>
            <xs:extension base="type_base_input_text_control">
                <xs:attribute name="Style" type="checkbox_style_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Styl zobrazení CheckBoxu.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_button = DfButton  -  Button obyčejný                                                       -->
    <xs:complexType name="type_button">
        <xs:annotation>
            <xs:documentation>Jednoduché tlačítko. Obsahuje Text, IconName, Action, HotKey atd.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="type_base_input_text_control">
                <xs:attribute name="ActionType" type="button_action_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Akce, kterou button provede.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ActionData" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Data pro akci (název funkce, atd).</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="HotKey" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Klávesa odpovídající buttonu, např. F6, Alt+Q, Shift+F3, atd...</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_drop_down_button = DfDropDownButton  -  DropDownButton obyčejný                             -->
    <xs:complexType name="type_drop_down_button">
        <xs:annotation>
            <xs:documentation>Tlačítko s rozbalovacím menu. Obsahuje navíc kolekci subtlačítek.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="type_button">
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="dropDownButton" type="type_subbutton">
                        <xs:annotation>
                            <xs:documentation xml:lang="cs-cz">Tlačítko v rozbalovacím menu DropDownButtonu.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_textbox = DfTextBox  -  TextBox obyčejný                                                    -->
    <xs:complexType name="type_textbox">
        <xs:complexContent>
            <xs:extension base="type_base_labeled_input_control">
                <xs:attribute name="EditMask">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Editační maska.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Alignment" type="content_alignment_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Zarovnání obsahu (textu) v definovaném prostoru prvku.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>        
    </xs:complexType>

	<!--    type_textbox = DfEditBox  -  Editor většího textu (Poznámka atd)                                -->
	<xs:complexType name="type_editbox">
		<xs:complexContent>
			<xs:extension base="type_base_labeled_input_control">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--    type_textboxbutton = DfTextBoxButton  -  TextBox s možností přidat malá tlačítka vpravo / vlevo  -->
    <xs:complexType name="type_textboxbutton">
        <xs:complexContent>
            <xs:extension base="type_textbox">
                <xs:sequence>
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="leftButton" type="type_subbutton">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Button zobrazený na levé straně TextBoxu.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="rightButton" type="type_subbutton">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Button zobrazený na pravé straně TextBoxu.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:sequence>
                <xs:attribute name="ButtonsVisibility" type="buttons_visibility_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Viditelnost buttonů v textboxu v závislosti na aktivitě TextBoxu.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

	<!--    type_image = DfImage  -  Zobrazovač obrázku (fotografie)                                         -->
	<xs:complexType name="type_image">
		<xs:complexContent>
			<xs:extension base="type_base_labeled_input_control">
				<xs:attribute name="ImageName" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Jméno obrázku. Obrázek bude vyhledán mezi ikonami (tedy 'pic/images/obrazek').</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ImageData" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Konstantní obsah obrázku dodaný ze serveru ve formě Base64.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--    type_stepprogress = DfStepProgress  -  StepProgress Bar = postupné kroky procesu                 -->
	<xs:complexType name="type_stepprogress">
		<xs:complexContent>
			<xs:extension base="type_base_control">
				<xs:sequence>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="step" type="type_subtextitem">
							<xs:annotation>
								<xs:documentation xml:lang="cs-cz">Jednotlivý krok v progresu, jeho statická vizualizace.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
				<xs:attribute name="EditStyleName" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Název editačního stylu. Může být prázdné, pokud budou zadány prvky 'step'.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--    type_combobox = DfComboBox  -  ComboBox obyčejný                                                 -->
    <xs:complexType name="type_combobox">
        <xs:complexContent>
            <xs:extension base="type_base_labeled_input_control">
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="comboItem" type="type_subtextitem">
                        <xs:annotation>
                            <xs:documentation xml:lang="cs-cz">Explicitně zadaný prvek v nabídce ComboBoxu.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="EditStyleName" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název editačního stylu. Může být prázdné, pokud budou zadány prvky comboItem.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Style" type="combobox_style_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Styl zobrazení ComboBoxu: S možností psaní, Pouze výběr hodnot, Výběr včetně zobrazení ikony.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_subbutton = DfSubButton  -  SubButton přidávaný do DropDownButtonu i do TextEditButotnu     -->
    <xs:complexType name="type_subbutton">
        <xs:complexContent>
            <xs:extension base="type_subtextitem">
                <xs:attribute name="ActionType" type="sub_button_action_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Akce, kterou tento sub-button provede.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ActionData" type="xs:string">
                    <xs:annotation>
						<xs:documentation xml:lang="cs-cz">Data pro akci (název akce pro Clipboard, název editoru, atd).</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_subtextitem = DfSubTextItem  -  Textový SubItem přidávaný do CheckBoxu (položka menu)       -->
    <xs:complexType name="type_subtextitem">
        <xs:complexContent>
            <xs:extension base="type_base">
                <xs:attribute name="Text" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Pevný text prvku (text Labelu, Buttonu, Checkboxu, ...). Může obsahovat formátovací funkce fm(), fmr(), fmdr().</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="IconName" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název ikony. Zobrazují ji pouze některé prvky.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <!--    DEFINICE BÁZOVÝCH TŘÍD                                                                           -->

    <!--    type_base_input_text_control = DfBaseInputTextControl - Bázová třída pro všechny samostatné controly s neměnným Textem a ikonou - Label, Button, CheckBox, ...  -->
    <xs:complexType name="type_base_input_text_control">
        <xs:complexContent>
            <xs:extension base="type_base_input_control">
                <xs:attribute name="Text" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Pevný text prvku (text Labelu, Buttonu, Checkboxu, ...). Může obsahovat formátovací funkce fm(), fmr(), fmdr().</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="IconName" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název ikony. Zobrazují ji pouze některé prvky.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Alignment" type="content_alignment_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Zarovnání obsahu (textu) v definovaném prostoru prvku.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

	<!--    type_base_labeled_input_control = DfBaseLabeledInputControl - Bázová třída pro všechny samostatné interaktivní controly, které mohou mít vedle políčka Label - TextBox, ComboBox, TokenEdit, ...  -->
	<xs:complexType name="type_base_labeled_input_control">
		<xs:complexContent>
			<xs:extension base="type_base_input_control">
				<xs:attribute name="Label" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Text, popisující obsah políčka.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
                <xs:attribute name="LabelPosition" type="label_position_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Umístění a zarovnání popisku (Labelu) vzhledem k souřadnicích controlu.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="LabelWidth" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Nejvyšší šířka prostoru pro Label.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--    type_base_input_control = DfBaseInputControl - Bázová třída pro všechny samostatné interaktivní controly - TextBox, Button, CheckBox, ComboBox, ...  -->
    <xs:complexType name="type_base_input_control">
        <xs:complexContent>
            <xs:extension base="type_base_control">
                <xs:attribute name="Required" type="required_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Příznak povinného políčka. Povinné políčko je barevně zvýrazněné.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
				<xs:attribute name="ColumnName" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Explicitně zadaný název sloupce (datového prvku), jehož data tento prvek zobrazuje. Běžně se nemusí zadávat, implicitně se data čtou a ukládají z/do datového sloupce se jménem podle atributu 'Name'. Pokud ale je třeba jeden atribut/vztah zobrazit na více místech formuláře, pak se pro tyto různé controly zadá různé 'Name' (musí být unikátní), a použije se pro ně shodné 'ColumnName'.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_base_control = DfBaseControl - Bázová třída pro všechny samostatné controly bez ohledu na jejich vlastní interaktivitu - tedy Label, Picture, Panel; a dále i pro interaktivní controly: TextBox, Button, CheckBox, ComboBox, ... Neslouží jako podklad pro Containery! Ty se odvozují od 'DfBase' a 'DfBaseArea'.  -->
    <xs:complexType name="type_base_control">
        <xs:complexContent>
            <xs:extension base="type_base">
				<xs:attribute name="StyleName" type="color_cup_enum">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Název stylu (jméno kalíšku), který určuje vzhled tohoto prvku.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Bounds" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice prvku v jednom políčku, v pořadí: X,Y,Width[,Height]. Alternativa hodnot X, Y, Width, Height.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Size" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Velikost prvku v jednom políčku, v pořadí: Width,Height. Alternativa hodnot Bounds, Width, Height.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="X" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Y" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Width" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Šířka prvku, ve Standardních pixelech (Zoom=100%). Je možné zadat souřadnice do jednoho atributu Bounds. Pouze do atributu Width lze zadat šířku v % z celkové šířky vyhrazeného sloupce. To platí pouze pro umístění do FlowLayout, nikoli FixedLayout.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Height" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Výška prvku, ve Standardních pixelech (Zoom=100%). Často je ignorována, pokud prvek má výšku danou přirozeně. Je možné zadat souřadnice do jednoho atributu Bounds.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
				<xs:attribute name="ColIndex" type="xs:int">
					<xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Index sloupce, na kterém je prvek umístěn v režimu FlowLayout. Ten se použije, pokud prvky nemají exaktně dané souřadnice, spolu s atributem 'ColumnWidths'. První sloupec má index 0. Zadáním indexu sloupce lze povinně přeskočit některé columny, anebo přeskočit na další řádek.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ColSpan" type="xs:int">
					<xs:annotation>
						<xs:documentation xml:lang="cs-cz">Počet sloupců, které prvek obsazuje v režimu FlowLayout. Ten se použije, pokud prvky nemají exaktně dané souřadnice, spolu s atributem 'ColumnWidths'.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
                <xs:attribute name="RowSpan" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Počet řádků, které prvek obsazuje v režimu FlowLayout. Ten se použije, pokud prvky nemají exaktně dané souřadnice.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="HPosition" type="position_enum">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Umístění prvku vodorovně v rámci sloupce v případě, kdy šířka prvku je menší než šířka sloupce. Řeší tedy zarovnání controlu ve sloupci, nikoli zarovnání obsahu v rámci controlu.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    type_base = DfBase - Bázová třída pro všechny prvky - controly i containery                      -->
    <xs:complexType name="type_base">
        <xs:attribute name="Name" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Identifikátor prvku v DataFormu = odpovídá jménu sloupce v načítaných datech. Pro panely a controly musí být jednoznačné v rámci DataFormu. Pro pomocné prvky (TextBoxButtony, ListItemy) musí být jednoznačné v rámci Owner prvku. Pokud více controlů má zobrazovat data jednoho datového sloupce, použije se atribut 'ColumnName'.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="State" type="control_state_enum">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Stav prvku (viditelnost, editovatelnost).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ToolTipTitle" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Titulek v okně ToolTip.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ToolTipText" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Text v okně ToolTip.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Invisible" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Výraz, při jehož splnění bude prvek neviditelný. Například: True (=neviditelný), pocet = 0 (pokud pocet bude 0, prvek bude neviditelný), ...</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    
