<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="DxDataFormat.Frm" targetNamespace="http://tempuri.org/DxDataFormat.Frm.xsd" elementFormDefault="qualified" xmlns="http://tempuri.org/DxDataFormat.Frm.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:annotation>
        <xs:documentation xml:lang="en">
            Part of Helios Nephrite, proprietary software, (c) Asseco Solutions, a. s.
            DxDataFormat template schema.
            All rights reserved.
        </xs:documentation>
    </xs:annotation>

    <!--    Struktura souboru, definujícího vzhled celého DataFormu    -->
    <xs:element name="template">
        <xs:complexType>
            <xs:sequence>
                <!--    V šabloně se mohou střídat libovolně elementy pageset a panel    -->
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="pageset" type="type_pageset" minOccurs="0" maxOccurs="unbounded" >
                        <xs:annotation>
                            <xs:documentation xml:lang="cs-cz">Jeden objekt s více záložkami (page)</xs:documentation>
                        </xs:annotation>
                    </xs:element>

                    <xs:element name="panel" type="type_panel" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation xml:lang="cs-cz">Jeden kontejner, obsahující prvky anebo další kontejnery</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:sequence>

            <xs:attribute name="FormatVersion" type="enum_format_version" default="4">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Formát tohoto souboru. Defaultní = 4</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="MasterWidth" type="xs:int">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Šířka oblasti DataFormu (Master) (tedy bez dynamických vztahů a dolních záložek)</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="MasterHeight" type="xs:int">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Výška oblasti DataFormu (Master) (tedy bez dynamických vztahů a dolních záložek)</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="TotalWidth" type="xs:int">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Celková šířka (tedy včetně dynamických vztahů a dolních záložek)</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="TotalHeight" type="xs:int">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Celková výška (tedy včetně dynamických vztahů a dolních záložek)</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="AutoLabelPosition" type="enum_label_pos" >
                <xs:annotation><xs:documentation xml:lang="cs-cz">Automaticky generovat labely atributů a vztahů, jejich umístění. Defaultní = None</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="DataSource" type="xs:string">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Jméno souboru, z něhož bude načtena definice dat</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="Messages" type="xs:string">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Jméno souboru, z něhož budou čteny překlady popisků a tooltipů</xs:documentation></xs:annotation>
            </xs:attribute>
            
            <xs:attribute name="UseNorisClass" type="xs:int">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Číslo třídy, pro kterou budou načteny popisky atributů a vztahů</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="AddUda" type="enum_bool">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Automaticky přidat záložku pro UDA atributy a vztahy. Nezadáno = True</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="UdaLabelPosition" type="enum_label_pos">
                <xs:annotation><xs:documentation xml:lang="cs-cz">Umístění labelů pro automaticky generované UDA atributy a vztahy. Defaultní = Top</xs:documentation></xs:annotation>
            </xs:attribute>
            
        </xs:complexType>
    </xs:element>
    
    
    <!--    Definice kontejnerů   -->

    <!--    PageSet               -->
    <xs:complexType name="type_pageset">
        <xs:sequence>
            <xs:element name="page" type="type_page" minOccurs="0" maxOccurs="unbounded" >
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Jedna záložka v řadě záložek</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>

        <xs:attribute name="X" type="xs:int">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%)</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Y" type="xs:int">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%)</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Width" type="xs:int">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Šířka prvku, ve Standardních pixelech (Zoom=100%)</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Height" type="xs:int">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Výška prvku, ve Standardních pixelech (Zoom=100%). Často je ignorována, pokud prvek má výšku danou přirozeně.</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Bounds" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Souřadnice prvku v jednom políčku, v pořadí: X,Y,Width[,Height]. Alternativa hodnot X, Y, Width, Height.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <!--    Page                  -->
    <xs:complexType name="type_page">
        <xs:complexContent>
            <xs:extension base="type_container">
                
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    Panel                 -->
    <xs:complexType name="type_panel">
        <xs:complexContent>
            <xs:extension base="type_container">



                <xs:attribute name="NestedTemplate" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název souboru, který obsahuje definici vnořené šablony</xs:documentation>
                    </xs:annotation>
                </xs:attribute>

                <xs:attribute name="X" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>

                <xs:attribute name="Y" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>

                <xs:attribute name="Width" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Šířka prvku, ve Standardních pixelech (Zoom=100%)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>

                <xs:attribute name="Height" type="xs:int">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Výška prvku, ve Standardních pixelech (Zoom=100%). Často je ignorována, pokud prvek má výšku danou přirozeně.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>

                <xs:attribute name="Bounds" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Souřadnice prvku v jednom políčku, v pořadí: X,Y,Width[,Height]. Alternativa hodnot X, Y, Width, Height.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    Container             -->
    <xs:complexType name="type_container">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="label" type="type_label" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Neinteraktivní textový popisek</xs:documentation>
                    </xs:annotation>
                </xs:element>

                <xs:element name="textbox" type="type_textbox" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Vstupní textové políčko</xs:documentation>
                    </xs:annotation>
                </xs:element>

                <xs:element name="textbox_button" type="type_textbox_button" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Vstupní textové políčko s buttony</xs:documentation>
                    </xs:annotation>
                </xs:element>

                <xs:element name="button" type="type_button" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Samostatné tlačítko</xs:documentation>
                    </xs:annotation>
                </xs:element>

                <xs:element name="combobox" type="type_combobox" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Rozbalovací nabídka typu ComboBox</xs:documentation>
                    </xs:annotation>
                </xs:element>

                
                <xs:element name="panel" type="type_panel" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Vnořený panel</xs:documentation>
                    </xs:annotation>
                </xs:element>

            </xs:choice>
        </xs:sequence>
        
        <xs:attribute name="Name" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Název odstavce / stránky. Jednoznačný v rámci DataFormu. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        
        <xs:attribute name="Invisible" type="xs:string" />

        <xs:attribute name="Title" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Pevný text prvku (text Labelu, Buttonu, Checkboxu, ...). Může obsahovat formátovací funkce fm(), fmr(), fmdr().</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="IconName" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Název ikony. Zobrazují ji pouze některé prvky.</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="ToolTipTitle" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Titulek v okně ToolTip</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="ToolTipText" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Text v okně ToolTip</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <!--    Definice jednotlivých controlů     -->

    <!--    Label                              -->
    <xs:complexType name="type_label">
        <xs:complexContent>
            <xs:extension base="type_control">
                <xs:attribute name="LabelMask"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    TextBox obyčejný                   -->
    <xs:complexType name="type_textbox">
        <xs:complexContent>
            <xs:extension base="type_control">
                <xs:attribute name="EditMask"/>
                <xs:attribute name="Required" type="enum_bool"/>
            </xs:extension>
        </xs:complexContent>        
    </xs:complexType>

    <xs:complexType name="type_combobox">
        <xs:complexContent>
            <xs:extension base="type_control">
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="comboItem" type="type_control">
                        <xs:annotation>
                            <xs:documentation xml:lang="cs-cz">Explicitně zadaný prvek v nabídce ComboBoxu</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="EditStyle" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Název editačního stylu. Může být prázdné, pokud budou zadány prvky comboItem.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="type_textbox_button">
        <xs:complexContent>
            <xs:extension base="type_textbox">
                <xs:sequence>
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="buttonRight" type="type_control">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Button zobrazený na pravé straně TextBoxu</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        
                        <xs:element name="buttonLeft" type="type_control">
                            <xs:annotation>
                                <xs:documentation xml:lang="cs-cz">Button zobrazený na levé straně TextBoxu</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:sequence>

                <xs:attribute name="ButtonsVisibility" type="enum_buttons_visibility">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Viditelnost buttonů v textboxu v závislosti na aktivitě TextBoxu</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    Button obyčejný                    -->
    <xs:complexType name="type_button">
        <xs:complexContent>
            <xs:extension base="type_control">
                <xs:attribute name="Action" type="type_button_action">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Akce, kterou button provede</xs:documentation>
                    </xs:annotation>
                </xs:attribute>

                <xs:attribute name="ButtonFunction" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Jméno funkce, kterou button vyvolá</xs:documentation>
                    </xs:annotation>
                </xs:attribute>

                <xs:attribute name="HotKey" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="cs-cz">Klávesa odpovídající buttonu, např. F6, Alt+Q, Shift+F3, atd...</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!--    Control - bázová třída             -->
    <xs:complexType name="type_control">
        <xs:attribute name="Name" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Identifikátor prvku, jméno sloupce v datech. Musí být jednoznačné v rámci DataFormu. Pro pomocné prvky (TextBoxButtony, ListItemy) musí být jednoznačné v rámci Owner prvku.</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="State" type="type_control_state">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Stav prvku (viditelnost, editovatelnost)</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Text" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Pevný text prvku (text Labelu, Buttonu, Checkboxu, ...). Může obsahovat formátovací funkce fm(), fmr(), fmdr().</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Alignment" type="enum_content_alignment">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Zarovnání obsahu (textu) v definovaném prostoru prvku</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        
        <xs:attribute name="IconName" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Název ikony. Zobrazují ji pouze některé prvky.</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="ToolTipTitle" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Titulek v okně ToolTip</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="ToolTipText" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Text v okně ToolTip</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Invisible" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Výraz, při jehož splnění bude prvek neviditelný. Například: True (=neviditelný), pocet = 0 (pokud pocet bude 0, prvek bude neviditelný), ...</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="X" type="xs:int">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%)</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Y" type="xs:int">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Souřadnice X prvku, ve Standardních pixelech (Zoom=100%)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        
        <xs:attribute name="Width" type="xs:int">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Šířka prvku, ve Standardních pixelech (Zoom=100%)</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Height" type="xs:int">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Výška prvku, ve Standardních pixelech (Zoom=100%). Často je ignorována, pokud prvek má výšku danou přirozeně.</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="Bounds" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="cs-cz">Souřadnice prvku v jednom políčku, v pořadí: X,Y,Width[,Height]. Alternativa hodnot X, Y, Width, Height.</xs:documentation>
            </xs:annotation>
        </xs:attribute>

    </xs:complexType>



    <!--    Definice enumů     -->
    <xs:simpleType name="enum_format_version">
        <xs:restriction base="xs:string">
            <xs:enumeration value="4" >
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">DataForm deklarovaný pro Nephrite</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3" >
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">DataForm deklarovaný pro Green, pokročilá verze. Nepoužívá se v této šabloně.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2" >
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">DataForm deklarovaný pro Green, střední verze. Nepoužívá se v této šabloně.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="1" >
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">DataForm deklarovaný pro Green, základní verze. Nepoužívá se v této šabloně.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="enum_label_pos">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Left" />
            <xs:enumeration value="Up" />
            <xs:enumeration value="None" />
            <xs:enumeration value="Right" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="enum_content_alignment">
        <xs:restriction base="xs:string">
            <xs:enumeration value="TopLeft" />
            <xs:enumeration value="TopCenter" />
            <xs:enumeration value="TopRight" />
            <xs:enumeration value="MiddleLeft" />
            <xs:enumeration value="MiddleCenter" />
            <xs:enumeration value="MiddleRight" />
            <xs:enumeration value="BottomLeft" />
            <xs:enumeration value="BottomCenter" />
            <xs:enumeration value="BottomRight" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="enum_buttons_visibility">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Default">
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Defaultní = jen v aktivním prvku (Focus nebo MouseOn), vyjma stavu Disabled</xs:documentation>
                </xs:annotation>
            </xs:enumeration>

            <xs:enumeration value="OnlyActive">
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Jen v aktivním prvku (Focus nebo MouseOn), vyjma stavu Disabled</xs:documentation>
                </xs:annotation>
            </xs:enumeration>

            <xs:enumeration value="VisibleAlways">
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Viditelné vždy</xs:documentation>
                </xs:annotation>
            </xs:enumeration>

            <xs:enumeration value="Invisible">
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Neviditelné</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="type_button_action">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Click">
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Odešle kliknutí na server</xs:documentation>
                </xs:annotation>
            </xs:enumeration>

            <xs:enumeration value="Update">
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Odešle Update dat na server</xs:documentation>
                </xs:annotation>
            </xs:enumeration>

            <xs:enumeration value="ClickCheckRequired">
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Ověří vyplněnost Required polí</xs:documentation>
                </xs:annotation>
            </xs:enumeration>

            <xs:enumeration value="RunFunction">
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Spustí funkci, název funkce je v atributu ButtonFunction</xs:documentation>
                </xs:annotation>
            </xs:enumeration>

            <xs:enumeration value="Close">
                <xs:annotation>
                    <xs:documentation xml:lang="cs-cz">Zavře okno</xs:documentation>
                </xs:annotation>
            </xs:enumeration>

        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="type_control_state">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Default" />
            <xs:enumeration value="Enabled" />
            <xs:enumeration value="ReadOnly" />
            <xs:enumeration value="Disabled" />
            <xs:enumeration value="Invisible" />
            <xs:enumeration value="SkipTab" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="enum_bool">
        <xs:restriction base="xs:string">
            <xs:enumeration value="True" />
            <xs:enumeration value="False" />
        </xs:restriction>
    </xs:simpleType>














    <!--    OBSOLETE   -->
    
    
    <xs:complexType name="type_column">
        <xs:sequence>
            <xs:element name="groupcolumn" type="column_e1" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attributeGroup ref="column_attr" />
    </xs:complexType>
    
    
    <xs:complexType name="column_e1">
        <xs:attributeGroup ref="column_attr" />
    </xs:complexType>
    
    
    <xs:attributeGroup name="column_attr">
        <xs:attribute name="TabIndex" type="xs:int" />
        <!-- <0, =0, >0 -->
        <xs:attribute name="EditMask">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="L10;D2;Z1;T1">
                        <!-- "L10;D2;Z1;T1" -->
                        <xs:annotation>
                            <xs:documentation>"NN NNN NNN.NN"</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="L10;D2;Z1;T0">
                        <!-- "L10;D2;Z1;T0" -->
                        <xs:annotation>
