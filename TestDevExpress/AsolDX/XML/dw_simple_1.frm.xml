<?xml version="1.0" encoding="utf-8"?>
<template xmlns="http://tempuri.org/DxDataFormat.Frm.xsd"
          FormatVersion="4"
          UseNorisClass="65"
          Messages="dw_common_messages.msg.xml"
          DataSource="dw_sample_form.data.xml"
		  AutoLabelPosition="Up"
          MasterWidth="530" MasterHeight="320"
          TotalWidth="750" TotalHeight="520"
		  AddUda="True" UdaLabelPosition="Up"
          ColumnWidths="200;300;100" >

    <panel Name="simple1" Title="Jednoduchý" ColumnsCount="2" AutoLabelPosition="Up" >
        <textbox Name="reference_subjektu"/>
        <textbox Name="nazev_subjektu"/>
        <textbox Name="dodavatel_refer"/>
        <textbox Name="dodavatel_nazev"/>
    </panel>

	<panel Name="middle1" Title="Střední" ColumnsCount="2" AutoLabelPosition="Up" >
		<textbox Name="reference_subjektu2" ColumnName="reference_subjektu" Width="120" Label="Číslo faktury"/>
		<textbox Name="nazev_subjektu2" ColumnName="nazev_subjektu" Width="300" Label="Variabilní symbol"/>
		<textbox Name="odberatel_refer2" LabelWidth="420" Label="Cílový odběratel zboží, které je zde fakturováno:" />
		<textbox Name="odberatel_nazev2" LabelPosition="None"/>
	</panel>

	<panel Name="middle2" Title="Zajímavý" ColumnWidths="120;250" AutoLabelPosition="Up" >
		<textbox Name="reference_subjektu3" ColumnName="reference_subjektu" Width="120" Label="Číslo faktury"/>
		<textbox Name="nazev_subjektu3" ColumnName="nazev_subjektu" Width="300" Label="Variabilní symbol"/>
		<textbox Name="kontakt_refer" LabelWidth="420" Label="Dodavatel zboží, které je zde fakturováno:" />
		<textbox Name="kontakt_nazev" LabelPosition="None"/>
		<button Name="open_contact" Bounds="380,0,60,50" Text="MSG001" ToolTipTitle="TIT001" ToolTipText="TIP001" IconName="pic/open_mail"/>
	</panel>

	<panel Name="maxim1" Title="Neobyčejný" ColumnWidths="120;250" AutoLabelPosition="Up" BackColorLight="#662211" BackColorDark="#220804" BackImageName="pic/back/angle_violet" >
		<textboxbutton Name="dokument" ColumnName="reference_subjektu" Label="Scan faktury" Bounds="25,30,400,20">
			<rightButton Name="open_doc" IconName="pic/opendoc"/>
			<rightButton Name="open_rec" IconName="pic/openrec"/>
		</textboxbutton>
		<image Name="logo" ImageName="pic/images/logo01" Bounds="25,60,320,240"/>
	</panel>




</template>
